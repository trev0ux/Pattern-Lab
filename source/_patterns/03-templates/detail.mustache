	</div>

	<div class="l l--two-col">

		<div class="l-main">
			{{> atoms-landscape }}
		</div><!--end .l-main-->

		<div class="l-sidebar">
			{{> organisms-text-passage }}
			{{# selectSection }}
			{{> molecules-select-field}}
